<div class="content_main">
<%= render :partial => 'form_filter' %>
  <div class="row">
   <% @posts.reverse.each do |post| %>
   
      <div class='post_div_small'>
         <h3><%= link_to post.title, post %></h3>
         <h5><%= post.address %></h5>
         <p>$ <%= post.price %></p>
         <p><%= post.content %></p>
      </div>
   <% end %>



  </div>
</div>
